{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "routs/email.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/email/(.*)",
        "dest": "routs/email.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  