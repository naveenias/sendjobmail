{
    "version": 2,
    "functions": {
        "api/email.js": {
          "includeFiles": "Naveen_Resume.pdf"
        }
    },
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
       {
      "src": "/api/email",
      "dest": "api/email.js"
       },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }